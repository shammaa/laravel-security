{
    "name": "shammaa/laravel-security",
    "description": "Comprehensive security package for Laravel with protection against SQL Injection, XSS, CSRF, and all common vulnerabilities",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "security",
        "sql-injection",
        "xss",
        "csrf",
        "authentication",
        "authorization",
        "rate-limiting",
        "file-upload",
        "security-headers",
        "monitoring",
        "policies"
    ],
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/auth": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelSecurity\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelSecurity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelSecurity\\LaravelSecurityServiceProvider"
            ],
            "aliases": {
                "Security": "Shammaa\\LaravelSecurity\\Facades\\Security"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

